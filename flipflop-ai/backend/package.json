{
  "name": "flipflop-backend",
  "version": "1.0.0",
  "description": "FlipFlop AI Backend with Supabase",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec tsx src/index.ts",
    "dev:prod": "nodemon --exec tsx src/index.ts",
    "build": "NODE_ENV=development tsc --project tsconfig.dev.json",
    "build:prod": "tsc",
    "start": "node dist/index.js",
    "test": "NODE_ENV=development jest --passWithNoTests",
    "lint": "eslint src --ext .ts",
    "db:types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/supabase.ts"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.14",
    "@octokit/rest": "^19.0.0",
    "@pinecone-database/pinecone": "^1.1.2",
    "@slack/events-api": "^3.0.1",
    "@slack/web-api": "^6.10.0",
    "@supabase/supabase-js": "^2.39.3",
    "axios": "^1.6.2",
    "bull": "^4.11.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "googleapis": "^128.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.0",
    "openai": "^4.24.1",
    "socket.io": "^4.6.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supabase": "^1.131.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
